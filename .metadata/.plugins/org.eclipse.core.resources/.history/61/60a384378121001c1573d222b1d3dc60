package com.joao.javaCaelum.exercicios;

public class Fatorial {

	public static void main(String[] args) {
		
		int fator = 1;
		int valor = 5; 

		for(int contador = 1; i <= valor; contador++ )
		{
		fator = fator * contador;
		}
		System.out.println( "O fatorial de " + valor + " é igual a " + fator );
		}
	}

