package com.joao.javaCaelum.exercicios;

public class soma1000 {
	public static void main(String[] args) {

		int i = 0;
		while (i < 1000) {
			for (int contador = 1; contador <= 50; contador++) {
				System.out.println("Posição: " + i + " Numero: " + contador);
				contador = contador + i;
			}
		}
	}
}
